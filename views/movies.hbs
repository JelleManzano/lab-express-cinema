<div>
    <h2>Ironhack Cinema</h2>
    <h4>Click on the movie to check the showtimes!</h4>
        {{#each moviesList}}
    <div id="movie-list">
        <img src="{{this.image}}" alt="movie-img" width="300px"> 
        <h2>{{this.title}}</h2>
         <a href="/movies/{{this._id}}">See more</a>
    </div>
        {{/each}}
</div>